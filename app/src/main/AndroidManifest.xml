<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bottombar">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BottomBar">
        <activity android:name=".ThirdActivity"></activity>
        <activity android:name=".SearchResult" />
        <activity android:name=".FragmentCamera" />
        <activity android:name=".FragmentCall" />
        <activity android:name=".FragmentSearch" />
        <activity android:name=".StockActivity" />
        <activity android:name=".activity_stock" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>